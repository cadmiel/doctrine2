{
    "name": "Doctrine",
    "description": "Destando doctrine 2",
    "type": "project",
    "homepage": "http://www.doctrine-project.org",
    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/orm": "2.5.*",
        "symfony/var-dumper": "^3.3",
        "doctrine/migrations": "^1.5"
    },
    "autoload":{
        "psr-4":{
            "App\\":"src"
        },
        "files": [
            "src/doctrine.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "require-dev":{
        "phpunit/phpunit": "^6.3"
    },
    "scripts":{
        "test": "vendor/bin/phpunit  --colors=always"
    }
}
